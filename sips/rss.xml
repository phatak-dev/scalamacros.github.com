---
layout: nil
---
<?xml version="1.0"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
  <channel>
    <title>Scala Macros SIPs</title>
    <link>{{ site.url }}</link>
    <atom:link href="{{ site.url }}/sips/rss.xml" rel="self" type="application/rss+xml" />
    <description>Scala Macros SIPs</description>
    <language>en-us</language>
    <pubDate>{{ site.time | date: "%a, %d %b %Y %H:%M:%S %Z" }}</pubDate>
    <lastBuildDate>{{ site.time | date: "%a, %d %b %Y %H:%M:%S %Z" }}</lastBuildDate>

    {% for entry in site.categories.sips %}
    <item>
      <title>{{ entry.title }}</title>
      <link>{{ site.url }}{{ entry.url }}</link>
      <pubDate>{{ entry.date | date: "%a, %d %b %Y %H:%M:%S %Z" }}</pubDate>
      <author>{{ site.email }}</author>
      <guid>{{ site.url }}{{ entry.id }}</guid>
      <description>{{ entry.content | xml_escape }}</description>
    </item>
    {% endfor %}
  </channel>
</rss>